{
  "name": "heredito",
  "version": "0.27.0",
  "description": "Extensive inheritance.",
  "main": "heredito.js",
  "support": "heredito.support.js",
  "deploy": "heredito.deploy.js",
  "browser": "heredito.deploy.js",
  "scripts": {
    "build:connect": "babel connect.module.js --out-file connect.js --source-maps",
    "build:inherit": "babel inherit.module.js --out-file inherit.js --source-maps",
    "build:heredito": "babel heredito.js --out-file heredito.support.js --source-maps",
    "build:all": "npm run build:connect && npm run build:inherit && npm run build:heredito",
    "build": "npm run build:all && webpack --progress --display-modules",
    "test": "mocha heredito-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volkovasystems/heredito.git"
  },
  "keywords": [
    "inheritance",
    "util.inherits",
    "inherit",
    "heredito"
  ],
  "author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
  "contributors": [
    "John Lenon Maghanoy <johnlenonmaghanoy@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volkovasystems/heredito/issues"
  },
  "homepage": "https://github.com/volkovasystems/heredito#readme",
  "dependencies": {
    "apiqe": "^0.1.0",
    "babel-runtime": "^6.23.0",
    "budge": "^0.28.0",
    "clazof": "^0.27.0",
    "diatom": "^0.27.0",
    "dictate": "^0.15.0",
    "een": "^0.18.0",
    "falzy": "^0.19.0",
    "firs": "^0.0.0",
    "harden": "^0.21.0",
    "kein": "^0.25.0",
    "leveld": "^0.4.0",
    "metod": "^0.7.0",
    "meton": "^0.0.0",
    "posp": "^0.3.0",
    "protype": "^0.21.0",
    "rder": "^0.0.0",
    "reclas": "^0.6.0",
    "sepby": "^0.2.0",
    "statis": "^0.8.0",
    "transpher": "^0.26.0",
    "truly": "^0.18.0",
    "wauker": "^0.14.0",
    "x10cv": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "source-map-loader": "^0.2.1",
    "webpack": "^2.5.1"
  }
}
